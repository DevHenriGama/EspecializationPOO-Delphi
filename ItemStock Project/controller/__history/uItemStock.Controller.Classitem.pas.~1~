unit uItemStock.Controller.Classitem;

interface

uses uItemStock.Controller.Interfaces;

  type
    TControllerItem = class(TInterfacedObject, IControllerItem)

      private

      public
        class function New : IControllerItem;
    end;

implementation

{ TControllerItem }

